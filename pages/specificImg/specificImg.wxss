/* pages/specificImg/specificImg.wxss */

.specificImg {
  color: #333;
  padding: 0 20rpx;
}

.specificImg .title {
  margin: 10rpx 0 0 0;
  text-align: center;
  font-size: 40rpx;
  padding: 10rpx 0;
  color: #333;
  font-weight: 650;
}

.specificImg .content {
  text-indent: 2em;
  font-size: 32rpx;
}

.specificImg .contactMessage {
  background: #f2f6f5;
  margin: 20rpx auto;
  font-weight: 600;
  padding: 20rpx 0;
  color: #0a8e71;
  border: 4rpx dotted #0a8e71;
  text-align: center;
  font-size: 28rpx;
}
